{
  "name": "analytics_query_tool",
  "private": true,
  "scripts": {
    "build:test": "NODE_ENV=test webpack --config webpack.config.js --mode none",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js --mode production",
    "build:development": "NODE_ENV=development webpack -w --config webpack.config.js --mode development",
    "lint": "eslint './**/*{.js,.jsx}'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:updateSnapshot": "jest --updateSnapshot"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@rails/webpacker": "^4.0.2",
    "autoprefixer": "^8.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "compression-webpack-plugin": "^2.0.0",
    "core-js": "^2.6.5",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "3.3.4",
    "es5-shim": "^4.5.9",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "flux": "^3.1.3",
    "fuse.js": "^3.2.0",
    "imports-loader": "^0.7.1",
    "intl": "^1.2.5",
    "jest-cli": "^21.2.1",
    "join-by-keys": "^1.0.0",
    "jquery": "^3.2.1",
    "jquery-visible": "^1.2.0",
    "js-yaml": "^3.8.2",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-on-rails": "11.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-react": "^7.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "less": "^2.7.3",
    "react-test-renderer": "15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js",
      "raf/polyfill"
    ],
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "app/components/*.jsx",
      "app/libs/*.jsx",
      "app/utils/*.jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
